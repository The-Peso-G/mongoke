{
    "builds": [
        { "src": "index/package.json", "use": "@now/static-build" },
        { "src": "docs/package.json", "use": "@now/static-build" }
    ],
    "routes": [
        {
            "src": "(.*)/docs",
            "status": 308,
            "headers": { "Location": "$1/docs/" }
        },
        {
            "src": "/docs/(.*)",
            "dest": "/docs/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index/$1"
        }
    ]
}
